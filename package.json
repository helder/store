{
  "name": "helder.store",
  "version": "0.0.0-alpha.3",
  "license": "MIT",
  "module": "./dist/esm/index.js",
  "main": "./dist/cjs/index.js",
  "exports": {
    ".": "./dist/esm/index.js",
    "./": "./dist/esm/helder/store/"
  },
  "typings": "./dist/esm/index.d.ts",
  "typesVersions": {
    "*": {
      "dist/esm/index.d.ts": [
        "./dist/esm/index.d.ts"
      ],
      "*": [
        "./dist/esm/helder/store/*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "lix download && symlink-dir . node_modules/helder.store",
    "test": "cross-env ANSICON=true lix run travix run",
    "test:ts": "lix build-js.hxml && uvu -r ts-node/register test/ts",
    "build": "lix build-js.hxml"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^5.4.1",
    "@types/sql.js": "^1.4.2",
    "better-sqlite3": "^7.4.1",
    "cross-env": "^7.0.3",
    "dev-null-cli": "^1.0.0",
    "dts2hx": "^0.15.3",
    "esm": "^3.2.25",
    "lix": "^15.11.4",
    "sql.js": "^1.5.0",
    "symlink-dir": "^5.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "uvu": "^0.4.1"
  }
}
